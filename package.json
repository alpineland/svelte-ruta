{
  "name": "svelte-ruta",
  "version": "0.0.0",
  "type": "module",
  "description": "Router for Svelte",
  "license": "MIT",
  "prettier": "@ydcjeff/prettier-config",
  "eslintConfig": {
    "extends": "@ydcjeff"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      }
    }
  },
  "dependencies": {
    "urlpattern-polyfill": "5.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.2",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.41",
    "@types/node": "17.0.29",
    "@ydcjeff/eslint-config": "0.3.1",
    "@ydcjeff/prettier-config": "0.3.1",
    "@ydcjeff/tsconfig": "0.3.1",
    "eslint": "8.14.0",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "prompts": "2.4.2",
    "semver": "7.3.7",
    "svelte": "3.47.0",
    "tslib": "2.4.0",
    "typescript": "4.6.3",
    "vite": "2.9.6",
    "vitest": "0.10.0"
  },
  "peerDependencies": {
    "svelte": "^3.47.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build_lib": "vite build --mode lib",
    "preview": "vite preview",
    "test": "pnpm run test_unit && pnpm run test_integraion",
    "test_unit": "vitest src",
    "test_integraion": "echo",
    "fmt": "prettier -w --ignore-unknown --plugin prettier-plugin-svelte .",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "preinstall": "git config core.hooksPath .githooks"
  }
}
