{
  "name": "svelte-ruta",
  "version": "0.0.0",
  "type": "module",
  "description": "Router for Svelte",
  "license": "MIT",
  "prettier": "@ydcjeff/prettier-config",
  "eslintConfig": {
    "extends": "@ydcjeff",
    "plugins": [
      "svelte3"
    ],
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "processor": "svelte3/svelte3"
      }
    ]
  },
  "files": [
    "src/lib"
  ],
  "types": "./src/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/lib/mod.js"
    }
  },
  "dependencies": {
    "urlpattern-polyfill": "4.0.3"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.44",
    "@types/node": "17.0.35",
    "@ydcjeff/eslint-config": "0.3.1",
    "@ydcjeff/prettier-config": "0.3.1",
    "@ydcjeff/tsconfig": "0.3.1",
    "eslint": "8.16.0",
    "eslint-plugin-svelte3": "4.0.0",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "svelte": "3.48.0",
    "vite": "2.9.9",
    "vitest": "0.12.9"
  },
  "peerDependencies": {
    "svelte": "^3.40.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "pnpm run test_unit && pnpm run test_integraion",
    "test_unit": "vitest src",
    "test_integraion": "echo",
    "fmt": "prettier -w --ignore-unknown --plugin prettier-plugin-svelte .",
    "check": "prettier -c --ignore-unknown --plugin prettier-plugin-svelte .",
    "lint": "eslint --ext .js,.svelte --ignore-path .gitignore .",
    "preinstall": "git config core.hooksPath .githooks"
  }
}
